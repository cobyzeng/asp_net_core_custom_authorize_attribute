<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <AspNetCoreHostingModel>InProcess</AspNetCoreHostingModel>
    <Company>creaftedforeveryone.com</Company>
    <Authors>Kaarthikeyan</Authors>
    <PackageLicenseUrl>https://github.com/kaarthikin/asp_net_core_custom_authorize_attribute/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/kaarthikin/asp_net_core_custom_authorize_attribute</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>Asp.Net Core, Custom Authorize Attribute, Custom Policy Provider</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.App" />
